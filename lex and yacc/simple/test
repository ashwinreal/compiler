decl
integer a;
integer b;
integer c[10];
enddecl
begin
a=3;
b=2;
if (a!=b) then
a=5;
b=4;
c[2]=2;
else
a=10;
b=11;
endif
a=c[1];
end



decl
integer a;
integer b;
integer c[10];
enddecl
begin
a=4;
b=1;
read a;
read b;
read c[1];
while (a!=b) do
b=b+1;
endwhile
write a;
write b;
write c[2];
end



decl
integer i;
integer j;
integer n;
integer A[10];
integer temp;
enddecl
begin
n=5;
A[0]=2;
A[1]=3;
A[2]=1;
A[3]=-1;
A[4]=4;

i=0;
while(i<n)do
	j=0;
	while(j<n)do
		if(A[j]>A[i])then
			temp = A[i];
			A[i] = A[j];
			A[j] = temp;
		endif;
		j=j+1;
	endwhile;
	i=i+1;
endwhile;
i=0;

end
